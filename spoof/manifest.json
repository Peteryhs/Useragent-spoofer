{
    "manifest_version": 2,
    "name": "User Agent Spoof",
    "version": "1.0",
    "description": "Spoofs the user agent for specified websites",
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "storage"
    ],
    "options_page": "options.html",
    "browser_action": {
      "default_popup": "options.html",
      "default_icon": {
        "48": "icons/border-48.png"
      }
    },
    "icons": {
      "48": "icons/border-48.png",
      "96": "icons/border-96.png"
    }
  }
  