{
  "manifest_version": 2,
  "name": "User Agent Spoof",
  "version": "1.0",
  "description": "Spoofs the user agent for specified websites",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "storage"
  ],
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "options.html"
  },
  "icons": {}
  
  }

